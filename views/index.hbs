<h1>Restaurants</h1>
<ul class="restaurants">
  {{#each restaurants}}
    <li class="restaurants-item">
      <img src="{{this.imageURL}}" class="restaurants-item-img" />
      <div class="restaurants-tem-name">{{this.name}}</div>
      <small class="restaurants-item-type">{{this.type}}</small>
    </li>
  {{/each}}
</ul>